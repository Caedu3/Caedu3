button{
	background-color: black;
	color: white;
	width: 100px;
	position: absolute;
}
.hidden{
	display: none;
}
#move{
	left:200px;
}
#change: hover {
	cursor:pointer;
	
}

$("#change").click(function(){
	$("#change").html("Sim");
	$("#move").show();
});

$("#move").hover(function(){
	change();
});

function change()
{
	var i = Math.floor(Math.random()*500)+1;
	var j = Math.floor(Math.random()*100)+1;
	$("#move").css({top: j+'px',left:       i+'px'});
